{
  "name": "projec",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "create-ddd-structure": "node ddd.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@humanwhocodes/config-array": "^0.13.0",
    "@humanwhocodes/object-schema": "^2.0.3",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.0.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "dotenv-expand": "12.0.1",
    "glob": "^11.0.1",
    "har-validator": "^5.1.5",
    "inflight": "^1.0.6",
    "module-alias": "^2.2.3",
    "mysql2": "^3.12.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "solr-client": "0.10.0-rc10",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.11",
    "@types/date-fns": "^2.6.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "moduleNameMapper": {
    "^@src/(.*)$": "<rootDir>src/$1",
    "^@app/(.*)$": "<rootDir>src/app/$1",
    "^@rest/(.*)$": "<rootDir>src/app/rest/$1",
    "^@interceptor/(.*)$": "<rootDir>src/app/interceptor/$1",
    "^@common/(.*)$": "<rootDir>src/context/common/$1",
    "^@search/(.*)$": "<rootDir>src/context/search/$1"
  },
  "_moduleAliases": {
    "@src": "./dist/src",
    "@app": "./dist/src/app",
    "@rest": "./dist/src/app/rest",
    "@interceptor": "./dist/src/app/interceptor",
    "@common": "./dist/src/context/common",
    "@search": "./dist/src/context/search"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "es5-ext"
    ],
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "es5-ext"
    ]
  }
}
